<h1>All Obituaries</h1>
  <table class="table table-striped table-bordered cart-table">
    <thead>
      <tr>
        <th scope="col">Image</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Age</th>
        <th scope="col">City</th>
        <th scope="col">State</th>
      </tr>
    </thead>
    <tbody>
      <% @obituaries.each do |obituary| %>
        <tr id="obit-<%= obituary.id %>">

          <td><%= link_to image_tag(obituary.image_url), "/obituaries/#{obituary.id}" %></td>
          <td><%= obituary.first_name %></td>
          <td><%= obituary.last_name %></td>
          <td><%= obituary.age %></td>
          <td><%= obituary.city %></td>
          <td><%= obituary.state %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
