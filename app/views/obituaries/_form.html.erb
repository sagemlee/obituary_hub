<%= form_for @obituary do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name  %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :age %>
  <%= f.number_field_tag :age, min: 0, max: 150 %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :description %>
  <%= f.text_field :description, :size => 150 %>

  <%= f.submit %>
<% end %>
